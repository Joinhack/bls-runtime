(use "typenames.witx")

(module $blockless_drivers
  (@interface func (export "blockless_open")
    (param $url string)
    (param $opts string)
    (result $error (expected $fd (error $errno)))
  )

  (@interface func (export "blockless_close")
    (param $fd $fd)
    (result $error (expected (error $errno)))
  )
)